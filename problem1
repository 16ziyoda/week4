#include <iostream>
using namespace std;

int main() {
    int x;

    // Ask user for input
    cout << "Enter an integer: ";
    cin >> x;

    // Check if the number is positive or negative
    if (x >= 0) {
        cout << "The number is positive." << endl;
    } else {
        cout << "The number is negative." << endl;
    }

    return 0;
}
