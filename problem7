#include <iostream>
using namespace std;

int main() {
    int angle1, angle2, angle3;
    cout << "Enter three angles of a triangle: ";
    cin >> angle1 >> angle2 >> angle3;
    if (angle1 > 0 && angle2 > 0 && angle3 > 0 && (angle1 + angle2 + angle3 == 180)) {
        cout << "Yes, a triangle can be formed with these angles." << endl;
    } else {
        cout << "No, a triangle cannot be formed with these angles." << endl;
    }

    return 0;
}
